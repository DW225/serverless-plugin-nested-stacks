    },
    "license": "MIT",
    "dependencies": {
        "bluebird": "^3.7.0"
    },
    "devDependencies": {
        "ava": "^5.1.0",
        "eslint": "^8.28.0",
        "eslint-config-standard": "^17.0.0",
        "eslint-plugin-import": "^2.26.0",
        "eslint-plugin-node": "^11.0.0",
        "eslint-plugin-promise": "^6.1.0",
        "jshint": "^2.13.6",
        "nyc": "^15.1.0",
        "serverless": "^2.72.0"
    },
    "peerDependencies": {
