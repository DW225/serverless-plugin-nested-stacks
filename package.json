{
    "name": "serverless-aws-nested-stacks",
    "main": "index",
    "version": "0.1.2",
    "description": "Serverless plugin to easily set up AWS cloudformation nested stacks!",
    "keywords": [
        "serverless",
        "node",
        "aws"
    ],
    "repository": {
